<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.landlord.android">

    <application
        android:name=".LandlordApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".views.EstatesList.EstatesListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.EstateDetails.EstateDetailsActivity" />
        <activity android:name=".views.EstateCreate.EstateCreateActivity"></activity>
        <activity android:name=".views.MessagesList.MessagesListActivity"></activity>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>